{
  "name": "lonesomedom",
  "version": "2.0.0",
  "description": "Standalone dom generator",
  "main": "index.js",
  "scripts": {
    "jscs": "jscs --config=node_modules/ivs-jssc/ivs-node.jscsrc index.js",
    "eslint": "eslint --config=node_modules/ivs-jssc/ivs-browser.eslintrc.js index.js",
    "checkall": "npm run jscs && npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ivsgroup/lonesomedom.git"
  },
  "keywords": [
    "standalone",
    "offline",
    "ucss",
    "uncss",
    "microcss",
    "portable",
    "dom"
  ],
  "author": "Francois Leurent <fleurent@ivsweb.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivsgroup/lonesomedom/issues"
  },
  "homepage": "https://github.com/ivsgroup/lonesomedom",
  "dependencies": {
    "microcss": "^1.3.3",
    "mout": "^1.0.0"
  },
  "devDependencies": {
    "ivs-jssc": "^2.1.3"
  }
}
